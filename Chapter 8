def full_name(first_name, last_name):
    """Returns a formatted full name."""
    return f"{first_name.title()} {last_name.title()}"

# Function calls with different names
person1 = full_name("Utkarsh", "Sharma")
person2 = full_name("Laurin", "Garcoks")
person3 = full_name("Pierce", "Lashley")

# Displaying the formatted names
print(person1)
print(person2)
print(person3)
